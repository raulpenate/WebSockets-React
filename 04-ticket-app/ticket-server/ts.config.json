{
    "compilerOptions": {
      "target": "es2022",
      "lib": [ "es2022", "DOM" ],
      "module": "es2022",
      "rootDir": "src",
      "allowJs": false,
      "outDir": "dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "noImplicitAny": true,
      "moduleResolution": "node16"
    }
  }